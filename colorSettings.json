{
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      // General strings
      {
        "scope": "string",
        "settings": {
          "foreground": "#b3b3b3" // Color for all strings
        }
      },
      // Object properties
      {
        "scope": "variable.other.property",
        "settings": {
          "foreground": "#059862" // Color for properties like length
        }
      },
      // Variables
      {
        "scope": "variable",
        "settings": {
          "foreground": "#39ac47" // Color for variables
        }
      },
      // Constants
      {
        "scope": "variable.other.constant",
        "settings": {
          "foreground": "#33993f" // Color for constants
        }
      },
      {
        "scope": "entity.name.function",
        "settings": {
          "foreground": "#9551a7" // Color for function names
        }
      },
      // Object properties
      {
        "scope": "variable.other.property",
        "settings": {
          "foreground": "#c58af9" // Color for properties like length
        }
      },
      // Object properties
      {
        "scope": "variable.other.property",
        "settings": {
          "foreground": "#2b98ff" // Color for properties like length
        }
      }
    ]
  }
}
